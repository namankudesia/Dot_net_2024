
@{
    ViewBag.Title = "GetCustomersInGermany";
}

<h2>GetCustomersInGermany</h2>

@model IEnumerable<Namanques1.Models.Customer>

<h2>Customers in Germany</h2>

<table>
    <thead>
        <tr>
            <th>Customer ID</th>
            <th>Company Name</th>
            <th>Contact Name</th>
            <th>City</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)
        {
            <tr>
                <td>@customer.CustomerID</td>
                <td>@customer.CompanyName</td>
                <td>@customer.ContactName</td>
                <td>@customer.City</td>
            </tr>
        }
    </tbody>
</table>